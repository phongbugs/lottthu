import {
Table,
Column,
Model,
DataType,
} from 'sequelize-typescript';

@Table({ tableName: '<%= modelName %>' })
    export class <%= modelName %> extends Model<<%= modelName %>> {
            <% attributes.forEach(attribute=> { %>
                @Column({
                type: DataType.<%= attribute.type %>,
                    allowNull: <%= attribute.allowNull %>,
                        })
                        <%= attribute.name %>!: <%= attribute.type==='STRING' ? 'string' : 'number' %>;
                                <% }); %>
                                    }
